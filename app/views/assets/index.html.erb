<% content_for :header_tags do %>
    <%= stylesheet_link_tag 'todos', :plugin => 'redmine_priorities_plugin' %>
    <%= stylesheet_link_tag 'todos_print', :plugin => 'redmine_priorities_plugin', :media => 'print' %>
<% end %>

<div id="project-assets">
  <h2><%= l(:assets_project_heading) %> <%= @project.name %></h2>
  <% if !@types.blank? %>
  <ul class="assets-list">
    <% for type in @types %>
      <li><%= link_to type, assets_by_type_path(:type => type, :project_id => params[:project_id]) %></li>
    <% end %>
  </ul>
  <% else %>
    
  <% end %>
</div>


